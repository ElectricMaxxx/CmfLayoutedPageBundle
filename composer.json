{
    "name": "ElectricMaxx/CmfLayoutedPageBundle",
    "description": "Will create a LayoutedPageBlock which will combine a grid layout and several block of the symfony-cmf",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "ElectricMaxxx",
            "email": "Maximilian.Berghoff@gmx.de"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "~2.4",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "doctrine/phpcr-bundle": "1.1.*",
        "doctrine/phpcr-odm": "1.1.*",
        "sonata-project/doctrine-phpcr-admin-bundle": "1.1.*",
        "symfony-cmf/core-bundle": "1.0.*",
        "symfony-cmf/content-bundle": "1.1.*",
        "symfony-cmf/menu-bundle": "1.1.*",
        "symfony-cmf/block-bundle": "1.1.*",
        "symfony-cmf/routing-bundle": "1.1.*",
        "symfony-cmf/media-bundle": "1.0.*"
    },
    "autoload": {
        "psr-0": { "Symfony\\Cmf": "src" }
    }
}